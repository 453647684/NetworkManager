"    dummy:         \n"
"\n"
"  SLAVE_OPTIONS:\n"
"    bridge:       [priority <0-63>]\n"
"                  [path-cost <1-65535>]\n"
"                  [hairpin yes|no]\n"
"\n"
"    team:         [config <file>|<raw JSON data>]\n"
"\n"
"  IP_OPTIONS:\n"
