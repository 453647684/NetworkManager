#, c-format
msgid "Name:  %s"
msgstr "Név: %s"

#: ../properties/nm-pptp.c:392
#, c-format
msgid "Remote:  %s"
msgstr "Távoli: %s"

#: ../properties/nm-pptp.c:395
#, c-format
msgid "Username:  %s"
msgstr "Felhasználói név: %s"

#: ../properties/nm-pptp.c:399
#, c-format
