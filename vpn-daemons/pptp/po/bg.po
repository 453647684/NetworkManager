#: ../auth-dialog/main.c:161
#, c-format
msgid "You need to authenticate to access the Virtual Private Network '%s'."
msgstr "Трябва да се идентифицирате, за да достъпите ВЧМ „%s“."

#: ../auth-dialog/main.c:162
msgid "Authenticate PPTP VPN"
msgstr "Идентифициране на ВЧМ по PPTP"

#: ../nm-pptp.desktop.in.h:1
msgid "Add, Remove, and Edit VPN Connections"
msgstr "Добавяне, изтриване и редактиране на връзки към ВЧМ"

#: ../nm-pptp.desktop.in.h:2
msgid "VPN Connection Manager (PPTP)"
msgstr "Управление на връзките към ВЧМ (за PPTP)"

#: ../properties/nm-pptp.c:79
msgid "PPTP Client"
msgstr "Клиент за PPTP"

#: ../properties/nm-pptp.c:387
msgid "The following PPTP connection will be created:"
msgstr "Ще бъде създадена следната връзка по PPTP:"

#: ../properties/nm-pptp.c:389
#, c-format
msgid "Name:  %s"
msgstr "Име: %s"

#: ../properties/nm-pptp.c:392
#, c-format
msgid "Remote:  %s"
msgstr "Отдалечен: %s"

#: ../properties/nm-pptp.c:395
#, c-format
msgid "Username:  %s"
msgstr "Потребителско име: %s"

#: ../properties/nm-pptp.c:399
#, c-format
msgid "Routes:  %s"
msgstr "Маршрути: %s"

#: ../properties/nm-pptp.c:403
#, c-format
msgid "Use MPPC Compression: %s"
msgstr "Използване на компресия MPPC: %s"

#: ../properties/nm-pptp.c:403 ../properties/nm-pptp.c:405
msgid "Yes"
msgstr "Да"

#: ../properties/nm-pptp.c:403 ../properties/nm-pptp.c:405
msgid "No"
msgstr "Не"

#: ../properties/nm-pptp.c:405
#, c-format
msgid "Use MPPE Encryption: %s"
msgstr "Използване на шифриране MPPE: %s"

#: ../properties/nm-pptp.c:408
msgid "The connection details can be changed using the \"Edit\" button."
msgstr "Настройките на връзката могат да се редактират с бутона „Редактиране“."

#: ../properties/nm-pptp.c:484
msgid "Cannot import settings"
msgstr "Настройките не могат да бъдат внесени"

#: ../properties/nm-pptp.c:486
#, c-format
msgid "The VPN settings file '%s' does not contain valid data."
msgstr "Файлът с настройки за ВЧМ „%s“ съдържа грешни данни."

#: ../properties/nm-pptp.c:504
msgid "Select file to import"
msgstr "Избор на файл за внасяне"

#. printf ("in impl_export\n");
#: ../properties/nm-pptp.c:626
msgid "Save as..."
msgstr "Запазване като..."

#: ../properties/nm-pptp.c:656
#, c-format
msgid "A file named \"%s\" already exists."
msgstr "Файл с име „%s“ вече съществува."

#: ../properties/nm-pptp.c:659
msgid "Do you want to replace it with the one you are saving?"
msgstr "Искате ли да го презапишете?"

#: ../properties/nm-pptp-dialog.glade.h:1
msgid "<b>Connection Name</b>"
msgstr "<b>Име на връзката</b>"

#: ../properties/nm-pptp-dialog.glade.h:2
msgid "<b>Required Information</b>"
msgstr "<b>Задължителна информация</b>"

#: ../properties/nm-pptp-dialog.glade.h:3
msgid "<i>example: 172.16.0.0/16 10.11.12.0/24</i>"
msgstr "<i>например: 172.16.0.0/16 10.11.12.0/24</i>"

#: ../properties/nm-pptp-dialog.glade.h:4
msgid "MS CHAP-v1/v2"
msgstr "MS CHAP-v1/v2"

#: ../properties/nm-pptp-dialog.glade.h:5
msgid ""
"Please enter the information provided by your system administrator below. Do "
"not enter your password here as you will be prompted when connecting."
msgstr ""
"По-долу въведете информацията, която системният ви администратор ви е дал. "
"Не въвеждайте паролата си - ще бъдете запитани за нея при свързване."

#: ../properties/nm-pptp-dialog.glade.h:6
msgid "Require MPPE encryption"
msgstr "Изискване на шифриране MPPE"

#: ../properties/nm-pptp-dialog.glade.h:7
msgid "Use MPPC compression"
msgstr "Използване на компресия MPPC"

#: ../properties/nm-pptp-dialog.glade.h:8
msgid "Username:"
msgstr "Потребителско име:"

#: ../properties/nm-pptp-dialog.glade.h:9
msgid ""
"Your password must be defined \n"
"in /etc/ppp/chap-secrets"
msgstr ""
"Паролата ви трябва да е указана във\n"
"файла „/etc/ppp/chap-secrets“"

#: ../properties/nm-pptp-dialog.glade.h:11
msgid "_Gateway:"
msgstr "_Шлюз:"

#: ../properties/nm-pptp-dialog.glade.h:12
msgid "_Import Saved Configuration..."
msgstr "_Внасяне на запазени настройки..."

#: ../properties/nm-pptp-dialog.glade.h:13
msgid ""
"_Name used to identify the connection to the private network, e.g. \"Campus "
"VPN\" or \"Corporate Network\""
msgstr ""
"_Име, което се ползва за различаването на връзката към тази мрежа, напр. "
"„Корпоративна мрежа“."

#: ../properties/nm-pptp-dialog.glade.h:14
msgid "_Only use VPN connection for these addresses"
msgstr "Да се използва само връзката през _ВЧМ за тези адреси"

